<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <property name="autoIncrement" value="true" dbms="mysql"/>
    <changeSet id="201710221618_authority" author="zengxn">
        <comment>authority schema</comment>
        <createTable tableName="authority">
            <column name="authority_id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="authority_name" type="varchar(50)">
                <constraints  nullable="false"/>
            </column>
            <column name="authority_code" type="varchar(100)">
                <constraints  nullable="false"/>
            </column>
            <column name="create_time" type="timestamp" defaultValueComputed="CURRENT_TIMESTAMP">
                <constraints  nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>